{
  "name": "soundm8_api",
  "version": "0.0.1",
  "description": "beat->mp3 tool+",
  "author": "SecretAI",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start:dev": "nest start --watch",
    "build": "nest build",
    "docker": "docker compose up",
    "start:prod": "node dist/main",
    "test": "jest",
    "prebuild": "rimraf dist",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migrate:create": "npm run typeorm -- migration:create ./src/migrations/migrations",
    "migrate:generate": "npm run typeorm -- migration:generate -d ./src/modules/typeorm/migrations/typeorm.data-source.ts ./src/migrations/migrations --timestamp -p",
    "migrate:run": "npm run typeorm migration:run -- -d ./src/modules/typeorm/migrations/typeorm.data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.1",
    "pg": "^8.7.3",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7",
    "ytdl-core": "^4.11.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/randomstring": "^1.1.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "jest": "28.1.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
